<script type="text/javascript">
    $(function () {



        $(".nav li:eq(0) a").addClass("n1Fir");

        var ulWid = $(".scztCon").width();
        liLen = $(".scrollBox li").length;
        scrBoxWid = ulWid * liLen;
        $(".scrollBox").css({ width: scrBoxWid + "px" });
        var thisindex = 1;
        $(".mcNext").click(function () {
            if (thisindex < liLen - 1) {
                thisindex++;
            } else {
                thisindex = 0;
            }
            scrFn(thisindex);
        });
        $(".mcPrev").click(function () {
            if (thisindex <= 0) {
                thisindex = liLen - 1;
            } else {
                thisindex--;
            }
            scrFn(thisindex);
        });
        //


        $.ajax({

            url: '../servers/servers.aspx?method=getNewstoBindshouye',

            success: function (text) {

                bind(text);

            }
        });

        $.ajax({

            url: '../servers/servers.aspx?method=getshiyantoBindshouye',

            success: function (text) {

                //bindshiyan(text);

            }
        });

    });

    function scrFn(thisindex) {
        $(".scrollBox").stop(true, true).animate({ left: (-thisindex * 300) + "px" })
    };

    function bind(text) {
        $("#gsim").empty();

        $("#gsxw").empty();
        var nurl = "news/pages/news.html";
        text = eval(text);
        $.each(text, function (i, item) {
            item.Ndate = item.Ndate.split('T')[0];

            if (item.title.length > 14) {
                item.title = item.title.substring(0, 14) + "..";
            }
            item.context = item.context.substring(0, 60) + "..";

            if (i == 0) {
                var ht = $("<dt><a href=\"" + nurl + "?id=" + item.ID + "\"><img src=\"" + item.imageurl + "\" alt=\"\" /></a></dt><dd><h2><a href=\"" + nurl + "?id=" + item.ID + "\">" + item.title + "</a></h2><div><a href=\"" + nurl + "?id=" + item.ID + "\">" + item.context + "</a></div></dd>");
                $("#gsim").append(ht);

            } else if (i == 1) {
                var ht = $("<li class=\"mLeftNwesListFir\"><span>" + item.Ndate + "</span><a href=\"" + nurl + "?id=" + item.ID + "\">" + item.title + "</a></li>");
                $("#gsxw").append(ht);

            }
            else {

                var ht = $("<li><span>" + item.Ndate + "</span><a href=\"" + nurl + "?id=" + item.ID + "\">" + item.title + "</a></li>");
                $("#gsxw").append(ht);
            }


        });
        $(".mLeftNwesList li").each(function () {
            $(this).hover(function () {
                $(".mLeftNwesList li").removeClass("mLeftNwesListFir");
                $(this).addClass("mLeftNwesListFir");
            });
        });
    }
    function bindshiyan(text) {
        $("#shiyannews").empty();


        text = eval(text);

        $.each(text, function (i, item) {
           
            var ht = $("<li class=\"scrollDiv\" ><a href=\"news/article.html?id="+item.ID+"\" class=\"mctitle\"><img src=\"Public/images/img2.jpg\" alt=\"\" /><h2>" + item.Title + "</h2></a><p>" + item.zhaiyao + "</p></li>");
            $("#shiyannews").append(ht);
        });

    }
    function imtz(id) {
        if (id == "1") {
            window.open("http://www.yunfangdata.com/index.html");
        }
        if (id == "2") {
            window.open("http://www.sym-fund.com");
        }
 if (id == "3") {
            window.open("http://www.zdpg.com.cn/");
        }
    }
    function kstz(id) {
        if (id == "1") {
            window.open("http://123.57.83.130:90/");
        } else if (id == "2") {

            window.open("http://online.yunfangdata.com/");
        }
        else if (id == "3") {

            location.href = "service/zwcx.html";
        }
        else if (id == "4") {

            location.href = "service/jdcx.html";
        }

    }
</script>
